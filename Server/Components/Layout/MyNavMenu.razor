        <MudDivider Class="my-2" />
        <MudNavLink Icon="@Icons.Material.Filled.Home" Href="/" Match="NavLinkMatch.Prefix">Home</MudNavLink>
        <MudNavLink Icon="@Icons.Material.Filled.LaptopMac" Href="/counter" Match="NavLinkMatch.Prefix">Counter</MudNavLink>
        <MudNavLink Icon="@Icons.Material.Filled.Dashboard" Href="/weather" Match="NavLinkMatch.Prefix">Weather</MudNavLink>
